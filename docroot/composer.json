{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/acquia_connector": "^1.19",
        "drupal/address": "~1.0",
        "drupal/address_checkout": "^1.0",
        "drupal/admin_toolbar": "^2.0",
        "drupal/advancedqueue": "^1.0-beta3",
        "drupal/better_exposed_filters": "^3.0-alpha6",
        "drupal/bg_img_field": "^1.4",
        "drupal/blazy": "^1.0-rc4",
        "drupal/block_class": "^1.0",
        "drupal/bootstrap": "^3.21",
        "drupal/captcha": "^1.0-beta4",
        "drupal/commerce": "^2.14",
        "drupal/commerce_cart": "^2.17",
        "drupal/commerce_cart_api": "^1.4",
        "drupal/commerce_checkout": "^2.17",
        "drupal/commerce_log": "^2.17",
        "drupal/commerce_number_pattern": "^2.17",
        "drupal/commerce_order": "^2.17",
        "drupal/commerce_payment": "^2.17",
        "drupal/commerce_paypal": "^1.0-beta6",
        "drupal/commerce_price": "^2.17",
        "drupal/commerce_product": "^2.17",
        "drupal/commerce_recurring": "^1.0-beta4",
        "drupal/commerce_store": "^2.17",
        "drupal/commerce_tax": "^2.17",
        "drupal/commerce_xquantity": "^1.0-beta5",
        "drupal/config_direct_save": "^1.0",
        "drupal/config_filter": "^1.5",
        "drupal/config_ignore": "^2.1",
        "drupal/consumers": "^1.9",
        "drupal/contribute": "^5.0-beta3",
        "drupal/copyright_block": "^1.0-rc2",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/cors": "^1.0",
        "drupal/crop": "^1.5",
        "drupal/csv_serialization": "^1.4",
        "drupal/devel": "^2.1",
        "drupal/devel_generate": "^2.1",
        "drupal/eck": "^1.0-alpha5",
        "drupal/email_registration": "1.0-rc7",
        "drupal/entity": "1.0-rc3",
        "drupal/entity_legal": "2.0-beta1",
        "drupal/entity_reference_revisions": "1.7",
        "drupal/field_group": "3.0-rc1",
        "drupal/file_mdm": "1.1",
        "drupal/flag": "4.0-beta1",
        "drupal/flexslider": "2.0-beta1",
        "drupal/flexslider_views": "^2.0@beta",
        "drupal/focal_point": "1.2",
        "drupal/fontawesome": "2.14",
        "drupal/fontawesome_menu_icons": "1.5",
        "drupal/fontyourface": "3.2",
        "drupal/http_client_manager": "^2.3",
        "drupal/image_captcha": "^1.0",
        "drupal/image_effects": "2.3",
        "drupal/inline_entity_form": "1.0-rc2",
        "drupal/interval": "1.3",
        "drupal/json_field": "1.0-rc3",
        "drupal/kint": "^2.1",
        "drupal/libraries": "3.0-alpha1",
        "drupal/masquerade": "2.0-beta2",
        "drupal/menu_export": "1.2",
        "drupal/menu_link_config": "1.0-alpha1",
        "drupal/menu_token": "1.0-alpha2",
        "drupal/paragraphs": "1.10",
        "drupal/pathauto": "^1.6",
        "drupal/persistent_login": "1.1",
        "drupal/physical": "1.0",
        "drupal/plaintext_encoder": "1.x-dev",
        "drupal/popup_field_group": "1.4",
        "drupal/printfriendly": "3.4",
        "drupal/profile": "^1.1",
        "drupal/purge": "3.0-beta9",
        "drupal/readmehelp": "1.0-beta4",
        "drupal/recaptcha": "2.4",
        "drupal/recurring_period": "1.0-alpha5",
        "drupal/rest_api_doc": "1.0-rc3",
        "drupal/restui": "1.17",
        "drupal/rules": "3.0-alpha5",
        "drupal/search_api": "1.14",
        "drupal/search_api_autocomplete": "1.2",
        "drupal/search_api_solr": "1.2",
        "drupal/share_everywhere": "1.1",
        "drupal/simple_block": "1.0-beta1",
        "drupal/simple_fb_connect": "3.1",
        "drupal/simple_oauth": "^4.4",
        "drupal/slick": "1.1",
        "drupal/slick_views": "2.0-alpha1",
        "drupal/social_api": "2.0-rc1",
        "drupal/social_auth": "2.0-rc1",
        "drupal/social_auth_facebook": "2.0-beta4",
        "drupal/social_auth_google": "2.0-beta4",
        "drupal/social_media_links": "2.6",
        "drupal/social_media_links_field": "^2.6",
        "drupal/stage_file_proxy": "1.0-rc2",
        "drupal/state_machine": "1.0-rc1",
        "drupal/token": "1.5",
        "drupal/token_filter": "1.1",
        "drupal/typed_data": "1.0-alpha3",
        "drupal/views_data_export": "1.0-beta3",
        "drupal/views_infinite_scroll": "1.6",
        "drupal/views_slideshow": "4.6",
        "drupal/viewsreference": "1.4",
        "drupal/webform": "5.5",
        "drupal/xnumber": "1.0-alpha3",
        "ext-openssl": "*"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
